<jqa-plugin:jqassistant-plugin
        xmlns:jqa-plugin="http://www.buschmais.com/jqassistant/core/plugin/schema/v1.0" name="Ruby">
    <description>Provides a scanner for Ruby files.</description>
    <model>
        <class>de.kontext_e.jqassistant.plugin.ruby.store.descriptor.AttributeDescriptor</class>
        <class>de.kontext_e.jqassistant.plugin.ruby.store.descriptor.AttributedDescriptor</class>
        <class>de.kontext_e.jqassistant.plugin.ruby.store.descriptor.ClassDescriptor</class>
        <class>de.kontext_e.jqassistant.plugin.ruby.store.descriptor.ConstantContainer</class>
        <class>de.kontext_e.jqassistant.plugin.ruby.store.descriptor.ConstantDescriptor</class>
        <class>de.kontext_e.jqassistant.plugin.ruby.store.descriptor.IncludeDescriptor</class>
        <class>de.kontext_e.jqassistant.plugin.ruby.store.descriptor.MethodContainer</class>
        <class>de.kontext_e.jqassistant.plugin.ruby.store.descriptor.MethodDescriptor</class>
        <class>de.kontext_e.jqassistant.plugin.ruby.store.descriptor.ModuleDescriptor</class>
        <class>de.kontext_e.jqassistant.plugin.ruby.store.descriptor.ParameterDescriptor</class>
        <class>de.kontext_e.jqassistant.plugin.ruby.store.descriptor.RequireDescriptor</class>
        <class>de.kontext_e.jqassistant.plugin.ruby.store.descriptor.RubyDescriptor</class>
        <class>de.kontext_e.jqassistant.plugin.ruby.store.descriptor.RubyFileDescriptor</class>
        <class>de.kontext_e.jqassistant.plugin.ruby.store.descriptor.SignatureDescriptor</class>
    </model>
    <scanner>
        <class>de.kontext_e.jqassistant.plugin.ruby.scanner.RubyScannerPlugin</class>
    </scanner>
</jqa-plugin:jqassistant-plugin>
